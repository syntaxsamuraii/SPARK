VAR a = 0;
VAR b = 0;

WRITE("Type a number 1 ");
READ(a);
WRITE("Type a number 2 ");
READ(b);

FN add() {
    WRITE("Added Answer");
    WRITE(a + b);
}

FN substract() {
    WRITE("Substracted Answer");
    WRITE(a - b);
}

FN multiply() {
    WRITE("Multiplied Answer");
    WRITE(a * b);
}

FN divide() {
    WRITE("Divided Answer");
    WRITE(a / b);
}
CALL add();
CALL substract();
CALL multiply();
CALL divide();
